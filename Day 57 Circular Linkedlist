// Define the Node class 
class Node { 
	int value; 
	Node next; 

	public Node(int value) { 
		this.value = value; 
	} 
} 

// Initialize the Nodes. 
Node one = new Node(3); 
Node two = new Node(5); 
Node three = new Node(9); 

// Connect nodes 
one.next = two; 
two.next = three; 
three.next = one; 
